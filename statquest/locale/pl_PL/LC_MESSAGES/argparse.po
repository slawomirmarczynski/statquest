#: argparse.py:296
msgid "usage: "
msgstr "użycie: "

#: argparse.py:860
msgid ".__call__() not defined"
msgstr "__call__() nie zdefiniowane"

#: argparse.py:1203
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "nieznany parser %(parser_name)r (propozycje: %(choices)s"

#: argparse.py:1263
msgid "argument \"-\" with mode %r"
msgstr "argument \"-\" z trybem %r"

#: argparse.py:1272
msgid "can't open '%(filename)s': %(error)s"
msgstr "nie mogę otworzyć ''%(filename)s': %(error)s"

#: argparse.py:1481
msgid "cannot merge actions - two groups are named %r"
msgstr "nie mogę połączyć akcji - dwie grupy są nazwane %r"

#: argparse.py:1519
msgid "'required' is an invalid argument for positionals"
msgstr "'required' jest niewłaściwym argumentem dla pozycyjnych"

#: argparse.py:1541
msgid "invalid option string %(option)r: must start with a character %(prefix_chars)r"
msgstr "niewłaściwy łańcuch opcji %(option)r: powinien się rozpoczynać znakiem %(prefix_chars)r"

#: argparse.py:1559
msgid "dest= is required for options like %r"
msgstr "dest= jest wymagany dla opcji takich jak %r"

#: argparse.py:1576
msgid "invalid conflict_resolution value: %r"
msgstr "niewłaściwa wartość conflict_resolution: %r"

#: argparse.py:1660
msgid "mutually exclusive arguments must be optional"
msgstr "wzajemnie wykluczające się argumenty muszą być opcjonalne"

#: argparse.py:1727
msgid "positional arguments"
msgstr "argumenty pozycyjne"

#: argparse.py:1728
msgid "optional arguments"
msgstr "argumenty opcjonalne"

#: argparse.py:1743
msgid "show this help message and exit"
msgstr "pokaż opis użycia i zakończ działanie"

#: argparse.py:1774
msgid "cannot have multiple subparser arguments"
msgstr "nie można mieć wielu argumentów subparsera"

#: argparse.py:1826 argparse.py:2337
msgid "unrecognized arguments: %s"
msgstr "nierozpoznany argument: %s"

#: argparse.py:1927
msgid "not allowed with argument %s"
msgstr "niedozwolone z argumentem %s"

#: argparse.py:1973 argparse.py:1987
msgid "ignored explicit argument %r"
msgstr "ignorowany jawny argument %r"

#: argparse.py:2094
msgid "the following arguments are required: %s"
msgstr "następujące argumenty są wymagane: %s"

#: argparse.py:2109
msgid "one of the arguments %s is required"
msgstr "jeden z argumentów %s jest wymagany"

#: argparse.py:2152
msgid "expected one argument"
msgstr "spodziewano się jednego argumentu"

#: argparse.py:2153
msgid "expected at most one argument"
msgstr "spodziewano się co najwyżej jednego argumentu"

#: argparse.py:2154
msgid "expected at least one argument"
msgstr "spodziewano się przynajmniej jednego argumentu"

#: argparse.py:2216
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "dwuznaczna opcja: %(option)s powinna pasować do %(matches)s"

#: argparse.py:2280
msgid "unexpected option string: %s"
msgstr "niespodziewany łańcuch w opcji: %s"

#: argparse.py:2477
msgid "%r is not callable"
msgstr "%r nie jest callable"

#: argparse.py:2494
msgid "invalid %(type)s value: %(value)r"
msgstr "niewłaściwa wartość %(type)s: %(value)r"

#: argparse.py:2505
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "zły wybór: %(value)r (wybierz z %(choices)s)"

#: argparse.py:2581
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: : %(message)s\n"

